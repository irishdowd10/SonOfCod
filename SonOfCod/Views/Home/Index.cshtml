@{
	ViewData["Title"] = "Home Page";
	Layout = "~/Views/Shared/_Layout.cshtml";
}




	<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
		<ol class="carousel-indicators">
			<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
			<li data-target="#myCarousel" data-slide-to="1"></li>
			<li data-target="#myCarousel" data-slide-to="2"></li>
			<li data-target="#myCarousel" data-slide-to="3"></li>
		</ol>
		<div class="carousel-inner" role="listbox">
			<div class="item active">
				<img src="~/images/Food1.1.jpg" 1 alt="Foo1.1" class="img-responsive" />
				<div class="carousel-caption" role="option">
					<p>
						Quality

					</p>
				</div>
			</div>
			<div class="item">
				<img src="~/images/Food2.jpg" alt="Food2" class="img-responsive" />
				<div class="carousel-caption" role="option">
					<p>
						Fresh

					</p>
				</div>
			</div>
			<div class="item">
				<img src="~/images/Food3.jpg" alt="Food3" class="img-responsive" />
				<div class="carousel-caption" role="option">
					<p>
						Lasting

					</p>
				</div>
			</div>
			<div class="item">
				<img src="~/images/Food4.jpg" alt="Food4" class="img-responsive" />
				<div class="carousel-caption" role="option">
					<p>
						Local


					</p>
				</div>
			</div>
		</div>
		<a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
			<span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
			<span class="sr-only">Previous</span>
		</a>
		<a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
			<span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
			<span class="sr-only">Next</span>
		</a>
	</div>

	<div class="row">
		<div class="col-md-3">
			<h2>Menu</h2>
			<ul>
				<li>Fish & Chips</li>
				<li>Vegetables </li>
				<li>Wine & Beer</li>
				
			</ul>
		</div>
		<div class="col-md-3">
			<h2>Northwest</h2>
			<ul>
				<li>Beauty</li>
				<li>Opneness</li>
				<li>Rugged</li>
			</ul>
		</div>
		<div class="col-md-3">
			<h2>Discovery</h2>
			<ul>
				<li>Family</li>
				<li>Fun</li>
				<li>Laughter</li>
			</ul>
		</div>
		<div class="col-md-3">
			<h2>Locations</h2>
			<ul>
				<li>Portland</li>
				<li>Seattle</li>
				<li>Eugene</li>

			</ul>
		</div>
	</div>


